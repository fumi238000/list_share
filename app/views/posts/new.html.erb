<h1>新規投稿</h1>

  <div class="text-left ml-3">
    <span>
      <%= link_to "戻る", user_path(current_user), class: 'text-dark'%>
   </span>
  </div>

<%= form_with model: @post, local: true do |form| %>
  <p><%= form.file_field :image, class: "btn btn-primary btn-block" %></p>
  <p><%= form.submit "送信", class: "btn btn-primary btn-block"%></p>
<% end %>